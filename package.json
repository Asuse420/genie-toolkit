  },
  "dependencies": {
    "argparse": "^1.0.10",
    "seedrandom": "^2.4.4",
    "thingtalk": "^1.4.2",
    "csv": "^5.0.0"
  },
  "devDependencies": {
    "coveralls": "^3.0.0",
    "eslint": "^5.3.0",
    "nyc": "^13.1.0",
